{
  "log": {
    "error": "./error.log",
    "loglevel": "warning"
  },
  "inbounds": [{
    "port": 12345,
    "protocol": "dokodemo-door",
    "settings": {
      "network": "tcp",
      "followRedirect": true
    }
  }, {
    "port": 1053,
    "protocol": "dokodemo-door",
    "settings": {
      "network": "udp",
      "address": "1.1.1.1",
      "port": 53
    }
  }],
  "outbounds": [{
    "protocol": "vmess",
    "tag": "proxy",
    "settings": {
      "vnext": [{
        "address": "==YOUR DOMAIN or SERVER ADDRESS==",
        "port": 443,
        "users": [{
          "id": "==YOUR USER ID==",
          "alterId": 4
        }]
      }]
    },
    "streamSettings": {
      "network" : "ws",
      "security": "tls",
      "wsSettings": {
        "path": "/==YOUR ENTRY PATH==/"
      }
    }
  }]
}
